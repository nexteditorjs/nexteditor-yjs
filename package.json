{
  "name": "@nexteditorjs/nexteditor-yjs",
  "version": "0.0.1",
  "files": [
    "./dist/index.umd.js",
    "./dist/index.d.ts"
  ],
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "start-server": "cd ../nexteditor-yjs-server && PORT=1234 node ./index.js",
    "start-client": "vite",
    "dev": "npm-run-all --parallel start-server start-client",
    "build": "vite build && tsc --emitDeclarationOnly",
    "dist": "vite build && tsc --emitDeclarationOnly --project tsconfig.dist.json"
  },
  "dependencies": {
    "@nexteditorjs/nexteditor-core": "0.0.20",
    "yjs": "^13.5.27"
  },
  "devDependencies": {
    "@nexteditorjs/nexteditor-table-block": "^0.0.2",
    "@nexteditorjs/nexteditor-input-handlers": "0.0.20",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.5.4",
    "vite": "^2.8.0",
    "y-websocket": "^1.3.18"
  }
}
